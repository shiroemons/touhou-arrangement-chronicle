# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Product {
  id: ID!
  title: String!
  shortTitle: String!
  productType: String!
  seriesNumber: Float!
}

type OriginalSong {
  id: ID!
  product: Product!
  title: String!
  composer: String!
  arranger: String!
  trackNumber: Int!
  duplicate: Boolean!
}

type Query {
  products: [Product!]!
  originalSongs: [OriginalSong!]!
}
